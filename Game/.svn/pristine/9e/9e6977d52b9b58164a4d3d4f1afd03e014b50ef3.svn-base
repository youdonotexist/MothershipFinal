using UnityEngine;
using System.Collections;

public class TimestampPoint
{
	public Vector3 point;
	public float timestamp;
	// Use this for initialization
	
	public TimestampPoint(Vector3 pt, float t) {
		Build (pt, t);
	}
	
	public void Build(Vector3 pt, float t) {
		point = pt;
		timestamp = t;
	}
}

